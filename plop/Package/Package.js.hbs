class {{ pascalCase name }} extends Package {
  static info: {
    description: '{{ description }}',
    weight: 0
  }
{{#if hasService}}
    services () {
      return {
        RandomNum: RandomNumService
      }
    }

{{/if}}
{{#if hasMiddleware}}
  middlewares () {
    return [
      {
        name: 'middleware-name',
        weight: 0,
        middleware: myMiddleware()
      }
    ]
  }

{{/if}}
{{#if hasDuck}}
  reducers () {
    return {
      counter: counterReducer
    }
  }
{{/if}}
}

export default {{ pascalCase name }}
